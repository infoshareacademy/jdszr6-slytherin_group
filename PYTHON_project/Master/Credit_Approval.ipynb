    "    # MEMBERS 13\n",
    "    dcc.Dropdown(\n",
    "        data_frame[\"MEMBERS\"].unique(),\n",
    "        placeholder = \"MEMBERS\",\n",
    "        id = \"members\"),\n",
    "    # AGE 14\n",
    "    dcc.Dropdown(\n",
